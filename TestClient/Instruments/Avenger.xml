<?xml version="1.0" encoding="utf-8" ?>
<Instruments xmlns="TsdLib.Instruments.xsd">
  <Instrument Name="Avenger" ConnectionType="Ssh" CommandDelay="200">
    <!--Discovery-->
    <IdQuery Response="Avenger"/>
    
    <!--Identification-->
    <ModelNumber Message="df" RegEx="(?&lt;=Board_Type::).*(?=\r\nBoot_Devices)"/>
    <SerialNumber Message="df" RegEx="(?&lt;=BSN::).*(?=\r\nBTMAC)"/>
    <FirmwareVersion Message="df" RegEx="(?&lt;=scmbundle::).*(?=\r\nscmbundle0)"/>
    
    <!--General-->
    <Query Name="GetImei" Message="cat /pps/system/nvram/deviceinfo" RegEx="(?&lt;=IMEI::).*(?=InProduction)" ReturnType="String" />
    <Query Name="BatteryStatus" Message="cat /pps/system/BattMgr/status" ReturnType="String"/>
  </Instrument>
</Instruments>