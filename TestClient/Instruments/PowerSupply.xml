<?xml version="1.0" encoding="utf-8" ?>
<Instruments xmlns="TsdLib.Instruments.xsd">
  <Instrument Name="PowerSupply" ConnectionType="Visa">
    <IdQuery Command="*IDN?\n" Response="MyInstrument, Model 12345"/>
    <ModelNumber Message="*IDN?\n"/>
    <SerialNumber Message="*IDN?\n"/>
    <FirmwareVersion Message="*IDN?\n"/>
    <Command Name="SetVoltage" Message="SET:VOLT {0}">
      <Parameter Name="voltage" Type="Double"/>
    </Command>
    <Query Name="GetCurrent" Message="GET:CURRENT?" ReturnType="Double" />
  </Instrument>
</Instruments>