<?xml version="1.0" encoding="utf-8" ?>
<Instruments xmlns="TsdLib.Instruments.xsd">
  <Instrument Name="Aos_WiFi" ConnectionType="Adb">

    <!--Discovery-->
    <IdQuery Response="Avengers"/>

    <ConnectionSetting Name="WiFi" ArgumentValue="" ArgumentType="String" />
    
    <!--Identification-->
    <ModelNumber Message="getprop | grep ro.product.board" RegEx="(?&lt;=: \[)\w+(?=\])"/>
    <!--<SerialNumber Message="getprop | grep ro.nvram.boardid.bsn" RegEx="(?&lt;=: \[)\d+(?=\])" Descriptor="BSN"/>-->
    <SerialNumber Message="cat /nvram/boardid/bsn &amp;&amp; echo ''" RegEx="\d+" Descriptor="BSN"/>
    <FirmwareVersion Message="getprop | grep ro.build.version.incremental" RegEx="(?&lt;=: \[)\w+(?=\])" Descriptor="OS Version"/>
   
    <Command Name="SetTcpPort" Message="setprop service.adb.tcp.port {0}">
      <Parameter Name="port" Type="Int32"/>
    </Command>
    <Command Name="ConnectViaWifi" Message="adb connect {0}">
      <Parameter Name="IpAddress" Type="String"/>
    </Command>
   
    <Query Name="GetWlanIpAddress" Message="ip -f inet addr show wlan0" RegEx="(?&lt;=inet )\d+\.\d+\.\d+\.\d+" ReturnType="String">
    </Query>

  </Instrument>
</Instruments>