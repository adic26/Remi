<?xml version="1.0" encoding="utf-8" ?>
<Instruments xmlns="TsdLib.Instruments.xsd">
  <Instrument Name="AIM4170C" ConnectionType="Visa" InitCommands="SYS;DCL:HMOD;*RST;*CLS;FORM:READ:DATA ASC">
	<IdQuery Command="V" TermChar="@" Response="4170C"/>
	
	<ModelNumber Message="V" RegEx="(?&lt;\s).*?(?=\s)" TermChar="@"/>
    <SerialNumber Message="V" RegEx="(?&lt;=DATE.*\s.*\s.*\s).*?(?=\s)" TermChar="@"/>
    <FirmwareVersion Message="V" RegEx="(?&lt;=ver ).*?(?=\s)" TermChar="@"/>
	
	<Command Name="CloseRelay" Message="K3"/>
	<Command Name="OpenRelay" Message="K0"/>
	
	<ByteQuery Name="Measure" Message="F{0}" ByteCount="72">
	  <Parameter Name="frequencyString" Type="String"/>
	</ByteQuery>
	
  </Instrument>
</Instruments>